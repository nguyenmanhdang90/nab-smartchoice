    <changeSet author="smartchoice-fapi" id="CREATE_TABLE_USER">
        <sql>
            CREATE TABLE `user` (
            id serial NOT NULL AUTO_INCREMENT,
            username varchar(50) NOT NULL,
            `password` varchar(255) NOT NULL,
            email varchar(355) NOT NULL,
